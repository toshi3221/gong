<?xml version="1.0" encoding="UTF-8"?>
<PreferenceScreen
	xmlns:android="http://schemas.android.com/apk/res/android">
	<EditTextPreference
		android:key="gong_interval"
		android:title="@string/gong_interval"
		android:summary="@string/gong_interval_summary"
		android:inputType="number"
		android:maxLength="5"
		android:defaultValue="300" />
	<EditTextPreference
		android:key="gong_times"
		android:title="@string/gong_times"
		android:summary="@string/gong_times_summary"
		android:inputType="number"
		android:maxLength="2"
		android:defaultValue="1" />
</PreferenceScreen>